pipeline {
    agent any
    stages {
        stage ("GIT Hub Checkout") {
            steps{
                echo "Source: Hello World"
                git credentialsId: 'Github', url: 'https://github.com/sonyvimal/myweb.git'
            }
        }
        stage ("Maven Build") {
            steps {
                echo "Trigger Maven Build"
                sh mvn clean package
            }
        }
    }
}
